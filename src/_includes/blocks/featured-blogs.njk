<div class="destination-cards swiper-slide">
  <img
    src="{{ post.data.image }}"
    alt="{{ post.data.title }}"
    loading="lazy"
    width="600"
    height="400"
  />
  <h3>{{ post.data.title }}</h3>
  <p>{{ post.data.excerpt }}</p>
  <a href="{{ post.url }}" class="btn3">Read article</a>
</div>

