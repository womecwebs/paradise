<!DOCTYPE html>
<html lang="en">
<head>
 <!-- Google tag (gtag.js) -->
    <script
      async
      src="https://www.googletagmanager.com/gtag/js?id=G-S1G5HQZXP2"
    ></script>
    <script>
      window.dataLayer = window.dataLayer || [];
      function gtag() {
        dataLayer.push(arguments);
      }
      gtag("js", new Date());

      gtag("config", "G-S1G5HQZXP2");
    </script>
    <meta
      name="google-site-verification"
      content="iMba3iNH_5vmxhCCOtq8EyrRUqMsPwOKYGRYRoJNIYU"
    />
    
  <meta charset="UTF-8" />
  <meta name="viewport" content="width=device-width, initial-scale=1.0" />

  {% include "seo.njk" %}
  {% include "schema.njk" %}


  <script src="https://identity.netlify.com/v1/netlify-identity-widget.js"></script>

  <link rel="stylesheet" href="/css/styles.css">
  <link rel="stylesheet" href="/css/utilities.css">
  <link rel="stylesheet" href="/css/next.css">

   <!-- Swiper CSS -->
    <link
      rel="stylesheet"
      href="https://cdn.jsdelivr.net/npm/swiper@11/swiper-bundle.min.css"
    />

     <!-- font for css -->
    <link
      href="https://fonts.googleapis.com/css2?family=Playfair+Display:wght@400;700&family=Montserrat:wght@300;500;700&display=swap"
      rel="stylesheet"
    />

    <!-- Favicon -->
    <link
      rel="apple-touch-icon"
      sizes="180x180"
      href="/images/icons/apple-touch-icon.png"
    />
    <link
      rel="icon"
      type="image/png"
      sizes="32x32"
      href="/images/icons/favicon-32x32.png"
    />
    <link
      rel="icon"
      type="image/png"
      sizes="16x16"
      href="/images/icons/favicon-16x16.png"
    />
    <link rel="manifest" href="/images/icons/site.webmanifest" />

    <!-- icons -->
    <link
      rel="stylesheet"
      href="https://fonts.googleapis.com/css2?family=Material+Symbols+Outlined"
    />
    <link
      href="https://unpkg.com/boxicons@2.1.4/css/boxicons.min.css"
      rel="stylesheet"
    />

    {% if isHome %}
  <style>
    .swiper-button-next,
    .swiper-button-prev {
      z-index: -1 !important;
      opacity: 0 !important;
      pointer-events: none !important;
    }

    @media screen and (min-width: 1099px) {
      .hero-container {
        max-width: 110rem;
      }
    }
  </style>
{% endif %}


</head>
<body>

  {% if page.filePathStem | startsWith("/blogs") %}
    {% include "nav-none.njk" %}
  {% else %}
    {% include "nav-default.njk" %}
  {% endif %}



  <main>
    {{ content | safe }}
  </main>

  
  {% include "blocks/newslatter-popup.njk" %}

  {% include "footer-default.njk" %}
    


<script>
  if (window.netlifyIdentity) {
    window.netlifyIdentity.on("init", user => {
      if (!user) {
        window.netlifyIdentity.on("login", () => {
          document.location.href = "/admin/";
        });
      }
    });
  }
</script>

    <!-- js files -->
    <script defer src="/js/where-next.js"></script>
    <script defer src="/js/core.js"></script>
</body>
</html>
